{
  "name": "Дмитрий Телицын",
  "private": true,
  "description": "Мое портфолио",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.4.0",
    "browser-sync": "2.27.10",
    "del": "^6.0.0",
    "gulp": "4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-libsquoosh": "^1.1.2",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-stacksvg": "^2.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-terser": "^2.1.0",
    "postcss": "^8.4.20",
    "postcss-csso": "^6.0.1",
    "postcss-scss": "^4.0.6",
    "stylelint": "^14.16.0",
    "stylelint-config-htmlacademy": "^1.1.4"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --customSyntax postcss-scss",
    "lint": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "npx gulp"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "engines": {
    "node": "^16"
  },
  "type": "module"
}
